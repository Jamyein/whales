<% if(theme.waline.enable) { %>
    <div id="waline"></div>
        <script type="module">
          import { init } from 'https://unpkg.com/@waline/client@v2/dist/waline.mjs';
          init({
            el: '#waline',
            serverURL: '<%- theme.waline.serverURL %>',
            dark: 'auto',
          });
        </script>
        <% } %>